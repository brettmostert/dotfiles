#!/bin/bash
#Generated by JetBrains Toolbox 3.2.0.65851 at 2026-02-01T17:26:47

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/brettmostert/Applications/DataGrip.app/Contents/MacOS/datagrip" $wait --args "${intellij_args[@]}"