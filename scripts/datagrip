#!/bin/bash
#Generated by JetBrains Toolbox 2.9.0.56191 at 2025-10-02T08:50:05

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/brettmostert/Applications/DataGrip.app/Contents/MacOS/datagrip" $wait --args "${intellij_args[@]}"